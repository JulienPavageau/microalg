(RAZ)
(Epaisseur 2)
(Remplissage (Liste 255 255 255))
(Declarer n x y r decalage De_type "nombre")
(Affecter_a n 1)
(Affecter_a x 0)
(Tant_que (<= n 5)
 Faire
    (Affecter_a credit_iterations 300)
    (Affecter_a x (+ (* n 50) 150))
    (Affecter_a y 200)
    (Affecter_a r 25)
    (Affecter_a decalage 0)
    (Tant_que (>= r 4)
     Faire
        (Contour (Liste (Entier@ 0 255) (Entier@ 0 255) (Entier@ 0 255)))
        (Cercle (Liste x y) r)
        (Affecter_a decalage (+ decalage (Entier@ -1 1)))
        (Si (< decalage -2)  Alors
            (Affecter_a decalage -2)
        )
        (Si (> decalage 2)  Alors
            (Affecter_a decalage -2)
        )
        (Affecter_a x (+ x decalage))
        (Affecter_a y (+ y 2))
        (Si (= 1 (Entier@ 1 6)) Alors
            (Affecter_a r (- r 2))
        )
    )
    (Affecter_a n (+ n 1))
)
