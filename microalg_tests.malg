(let (Goods 0 Bads 0)
	(mapc
		'((BoolExpr)
			(if (eval BoolExpr)
				(inc 'Goods)
				(inc 'Bads) (println (pack "Return value for " (sym BoolExpr) " was " (sym (eval (caddr BoolExpr))) ".")) ) )
		(list
			'(= "0.1.0" version)
            # Affichage
			'(= "Bonjour tout le monde !" (prog (Afficher "Bonjour tout le monde !") *StdOut))
            # Gestion des variables
            '(let (x NIL) (= 0 (prog (setq x 0) x)))
            '(let (x NIL) (= 0 (prog (Affecter_a x 0) x)))
            # Structure conditionnelle
            '(= 1 (Si Vrai 1 0))
            '(= 0 (Si Faux 1 0))
			) )
	(println (pack "Tests: " (+ Goods Bads) ", Errors: " Bads)) )
(bye)
